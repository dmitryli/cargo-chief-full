CREATE TABLE CC_CARRIER_INFO (ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY, BUSINESS_NAME VARCHAR(128),
FIRM_MC VARCHAR(128), HQ_CITY VARCHAR(128), HQ_STATE VARCHAR(128), HQ_ZIP INT, FLEET_SIZE INT, FLEET_AGE INT);


CREATE TABLE CC_CAPACITY_INFO (ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY, BUSINESS_NAME VARCHAR(128),
FIRM_MC VARCHAR(128), ORIGIN_CITY VARCHAR(128), ORIGIN_STATE VARCHAR(128), ORIGIN_ZIP INT, DEST_CITY VARCHAR(128),
DEST_STATE VARCHAR(128), DEST_ZIP INT, MINUTE_AGE INT);

